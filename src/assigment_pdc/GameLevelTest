import org.junit.After;
import org.junit.AfterClass;
import org.junit.Test;
import static org.junit.Assert.*;
import org.junit.Before;
import org.junit.BeforeClass;

public class GameLevelTest {
    
    public GameLevelTest() {
    }
    
    @BeforeClass
    public static void setUpClass() {
    }
    
    @AfterClass
    public static void tearDownClass() {
    }
    
    @Before
    public void setUp() {
    }
    
    @After
    public void tearDown() {
    }

    /**
     * Test of selectWord method, of class GameLevel.
     */
    @Test
public void testSelectWord() {
    System.out.println("selectWord");
    
    GameLevel instance = new GameLevel(1, new Words());
    
    String result = instance.selectWord();
    
    System.out.println("Selected word: " + result);
    
    assertNotNull(result);
    assertTrue("Selected word should have a length greater than 0", result.length() > 0);
}

}
